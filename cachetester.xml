<?xml version="1.0" encoding="utf-8"?>
<install version="2.5"
         type="plugin"
         group="system"
         method="upgrade">
	<name>System - Cache Tester</name>
	<files>
		<filename plugin="cachetester">cachetester.php</filename>
		<folder>elements</folder>
	</files>

	<params addpath="/plugins/system/elements">
		<param type="diagnostic" />
		<param name="cache"
		       type="list"
		       default="0"
		       label="Enable Cache"
		       description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
		<param name="cacheage"
		       type="text"
		       default="15"
		       label="Cache Age"
		       description="" />
		<param name="showdiagnostic"
		       type="list"
		       default="0"
		       label="Show Diagnostic"
		       description="">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>

</install>